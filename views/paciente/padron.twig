{% extends "layout.twig" %}

{% block content %}

	<div class="agregar_paciente">
		<h4>Importar padr√≥n</h4>

		{% if responseMessage %}

				<script>
				 

					Swal.fire({
						icon: 'success',
  						title: 'Padron importado con exito',
						text: '{{ responseMessage }}'
					}).then(() => {
  window.location = "/fuerarango/pacientes";
});;

					
				</script>

		{% endif %}

		<form action="procesarPadron" method="post" enctype="multipart/form-data">

			<div class="form-group">

				<input type="file" name="padron" id="padron" class="form-control">
			</div>


			<div class="form-group">
				<input type="submit" value="Procesar" class="btn btn-primary">

				<a href="/fuerarango/pacientes">
					<button type="button" class="btn btn-secondary">Cancelar</button>
				</a>
			</div>

		</form>
	</div>

{% endblock %}
